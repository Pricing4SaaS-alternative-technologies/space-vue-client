{
  "name": "@tu-org/space-vue-client",
  "type": "module",
  "version": "0.1.0",
  "description": "Vue client for SPACE-style feature gating using pricing tokens (token-only or connected mode)",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pricing4SaaS-alternative-technologies/space-vue-client"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "test": "vitest",
    "lint": "echo \"add eslint if needed\"",
    "prepare": "npm run build",
    "clean:install": "rm -rf node_modules dist && npm install",
    "clean:install:win": "Remove-Item -Recurse -Force node_modules,dist -ErrorAction SilentlyContinue; npm install"
  },
  "keywords": [
    "vue",
    "feature-flags",
    "feature-gating",
    "pricing",
    "saas",
    "token",
    "space"
  ],
  "license": "MIT",
  "author": "Daniel Benito Hidalgo danbenhid@alum.us.es Pablo Jesus Castellanos CompaÃ±a pabcascom@alum.us.es Fernando Jose de Celis Hurtado ferdehur@alum.us.es",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "tiny-emitter": "^2.1.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0",
    "vitest": "^1.2.0",
    "@types/tiny-emitter": "^2.0.3",
    "@types/socket.io-client": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}