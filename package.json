{
  "name": "@npm_team/space-vue-client",
  "version": "1.3.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Vue 2 client for SPACE-style feature gating using pricing tokens",
  "main": "dist/space-vue-client.umd.js",
  "module": "dist/space-vue-client.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Pricing4SaaS-alternative-technologies/space-vue-client"
  },
  "exports": {
    ".": {
      "import": "./dist/space-vue-client.es.js",
      "require": "./dist/space-vue-client.umd.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "rollup -c rollup.config.mjs -w",
    "build": "rollup -c rollup.config.mjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types --skipLibCheck",
    "build:all": "npm run build && npm run build:types",
    "test": "vitest",
    "clean": "rd /s /q dist",
    "clean:win": "rmdir /s /q dist",
    "prepack": "npm run build && npm run build:types",
    "prepublishOnly": "npm run build && npm run build:types"
  },
  "keywords": [
    "vue",
    "vue2",
    "feature-flags",
    "feature-gating",
    "pricing",
    "saas",
    "token",
    "space"
  ],
  "license": "MIT",
  "author": "Daniel Benito Hidalgo danbenhid@alum.us.es Pablo Jesus Castellanos CompaÃ±a pabcascom@alum.us.es Fernando Jose de Celis Hurtado ferdehur@alum.us.es",
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^2.5.0 || ^2.6.0 || ^2.7.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "jwt-decode": "^4.0.0",
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.0.0",
    "esbuild": "^0.20.0",
    "rollup": "^3.29.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-vue": "^5.1.9",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vitest": "^0.34.0",
    "vue": "^2.7.14",
    "vue-template-compiler": "^2.7.14"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
